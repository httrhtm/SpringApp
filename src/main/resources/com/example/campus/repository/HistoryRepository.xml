<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
	"-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<!-- namespace：mapperインターフェースのパッケージ付きクラス名を指定 -->
<mapper namespace="com.example.campus.repository.HistoryRepository">

	<!-- 新規作成 -->
	<insert id="create" parameterType="String">
		insert into histories (users_id, point, created_at) values (#{users_id}, #{point}, current_timestamp())
	</insert>
</mapper>